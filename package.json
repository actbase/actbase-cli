  "dependencies": {
    "chalk": "^2.4.2",
    "commander": "^3.0.2",
    "inquirer": "^7.0.4",
    "lodash": "^4.17.15",
    "node-fetch": "^2.3.0",
    "open": "^7.0.2",
    "plist": "^3.0.1",
    "select-shell": "^1.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/preset-env": "^7.6.2",
    "prettier": "^1.19.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actbase/actbase-cli.git"
  },
  "bugs": {
